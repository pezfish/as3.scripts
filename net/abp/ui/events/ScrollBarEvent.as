package net.abp.ui.events {	import flash.events.Event;		public class ScrollBarEvent extends Event {		public static const UPDATE:String = "update";		public static const SCROLL_START:String = "start";		public static const SCROLL_STOP:String = "stop";		public var scroll_value:Number;				public function ScrollBarEvent(_type:String, _value:Number = 0) {			super(_type);			scroll_value = _value;		}	}}