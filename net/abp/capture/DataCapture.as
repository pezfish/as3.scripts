package net.abp.capture {	//import	import flash.net.URLRequest;	import flash.net.URLRequestMethod;	import flash.net.sendToURL;	import flash.net.URLVariables;		public class DataCapture {		//var		private static var _data:Array = [];				public function DataCapture() {			//init				}				public static function store(key:String, info:Object):void {			_data[key] = info;		}				public static function retrieve(key):Object {			return _data[key];		}				public static function clear():void {			_data = [];		}						public static function post(url:String):void {			var _request:URLRequest = new URLRequest(url);			var _variables:URLVariables = new URLVariables();            for (var key:String in _data) {				_variables[key] = _data[key];			}			_request.method = "POST";			_request.data = _variables;            sendToURL(_request);						_request = null; //--- cleanup			_variables = null; //--- cleanup		}	}	}